\mode<presentation>

% Theme colors

\definecolor{primary}{HTML}{7CBF56}
\definecolor{secondary}{HTML}{748699}
\definecolor{accent}{HTML}{003A70}
\definecolor{alert}{HTML}{DA291C}
\definecolor{onPrimary}{HTML}{000103}
\definecolor{onSecondary}{HTML}{FEFEFE}

% Settings
\setbeamercolor*{title page header}{fg=accent}
\setbeamercolor*{author}{fg=primary}
\setbeamercolor*{date}{fg=accent}
\setbeamercolor*{item}{fg=accent}
\setbeamercolor{alerted text}{fg=alert}
\setbeamercolor{frametitle}{fg=accent}
\setbeamercolor{section in head/foot}{fg=accent, bg=primary}
\setbeamercolor{subsection in head/foot}{fg=accent, bg=primary}
\mode
<all>
